
(defun c:atlo () 
    (setvar "cmdecho" 0)
    (cmd "layout" "s" "")
    (cmd "layout" "t" "./A3_title_1.dwg" "A3")
    (cmd "layout" "d" (getvar "ctab"))
    (cmd "zoom" "e")
    (cmd "mspace" "zoom" "e")
    (c:scset)
    (cmd "pspace" "zoom" "e")
    (cmd "layout" "r" (getvar "ctab") "A3")
    (setvar "cmdecho" 1)
    (princ)
)
(princ "loaded. C:atlo")
(princ)
